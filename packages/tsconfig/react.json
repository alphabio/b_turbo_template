{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React TypeScript Config",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "target": "es6",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true
  }
}
